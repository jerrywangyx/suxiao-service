{
  "name": "daga-clone",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node -r ./scripts/chunk-require-patch.cjs ./node_modules/next/dist/bin/next dev",
    "prebuild": "node scripts/generate-homepage-content.cjs",
    "build": "node -r ./scripts/chunk-require-patch.cjs ./node_modules/next/dist/bin/next build",
    "start": "node -r ./scripts/chunk-require-patch.cjs ./node_modules/next/dist/bin/next start",
    "cloudflare:build": "node scripts/generate-homepage-content.cjs && npx @cloudflare/next-on-pages",
    "cloudflare:deploy": "npx wrangler pages deploy .vercel/output/static --project-name=wangyongxin --commit-dirty=true",
    "vercel:deploy": "vercel --prod"
  },
  "dependencies": {
    "@vercel/postgres": "^0.10.0",
    "bootstrap": "^5.3.3",
    "next": "15.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "wrangler": "^4.51.0"
  }
}
